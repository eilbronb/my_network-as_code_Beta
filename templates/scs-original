set vsys vsys1 ssl-tls-service-profile SCS9-Customer-VPN protocol-settings max-version max
set vsys vsys1 ssl-tls-service-profile SCS9-Customer-VPN certificate scs9_cert
set vsys vsys1 ssl-tls-service-profile SCS9-Admin-VPN protocol-settings min-version tls1-2
set vsys vsys1 ssl-tls-service-profile SCS9-Admin-VPN protocol-settings max-version max
set vsys vsys1 ssl-tls-service-profile SCS9-Admin-VPN certificate scs9-admin_cert

set vsys vsys1 authentication-profile SCS9-SAML multi-factor-auth mfa-enable no
set vsys vsys1 authentication-profile SCS9-SAML method saml-idp server-profile scs9.synopsys.com
set vsys vsys1 authentication-profile SCS9-SAML method saml-idp attribute-name-username username
set vsys vsys1 authentication-profile SCS9-SAML allow-list all
set vsys vsys1 authentication-profile SCS9-SAML username-modifier %USERINPUT%
set vsys vsys1 authentication-profile SCS9-admin-SAML multi-factor-auth mfa-enable no
set vsys vsys1 authentication-profile SCS9-admin-SAML method saml-idp server-profile scs9-admin.synopsys.com
